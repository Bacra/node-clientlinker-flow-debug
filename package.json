    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-brcjs": "^0.2.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Bacra/node-clientlinker-flow-debugger.git"
